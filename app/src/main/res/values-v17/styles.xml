<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <!-- Dividers -->
    <style name="HorizontalDivider" tools:ignore="UnusedResources">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/divider_height</item>
        <item name="android:background">?android:attr/listDivider</item>
        <item name="android:importantForAccessibility">no</item>
    </style>

    <style name="VerticalDivider" tools:ignore="UnusedResources">
        <item name="android:layout_width">@dimen/divider_height</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?android:attr/listDivider</item>
        <item name="android:importantForAccessibility">no</item>
    </style>

</resources>